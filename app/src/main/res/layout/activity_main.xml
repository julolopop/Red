<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.usuario.red.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/Eje1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Conexión http"
        app:layout_constraintBottom_toTopOf="@+id/Eje2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/Eje2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Conexión Asincrona"
        app:layout_constraintBottom_toTopOf="@+id/Eje3"
        app:layout_constraintTop_toBottomOf="@+id/Eje1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/Eje3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AAHC"
        app:layout_constraintBottom_toTopOf="@+id/Eje4"
        app:layout_constraintTop_toBottomOf="@+id/Eje2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/Eje4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ConexiónVolly"
        app:layout_constraintBottom_toTopOf="@+id/Eje5"
        app:layout_constraintTop_toBottomOf="@+id/Eje3"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/Eje5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descarga"
        app:layout_constraintBottom_toTopOf="@+id/Eje6"
        app:layout_constraintTop_toBottomOf="@+id/Eje4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/Eje6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subida"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Eje5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

</android.support.constraint.ConstraintLayout>
